{
    "access-log-file": null,
    "algo": "cryptonight",
    "api": {
        "port": 0,
        "access-token": null,
        "worker-id": null,
        "ipv6": false,
        "restricted": true
    },
    "background": false,
    "bind": [
        "0.0.0.0:3333"
    ],
    "colors": true,
    "donate-level": 1,
    "log-file": "{{ log }}",
    "mode": "nicehash",
    "retries": 2,
    "retry-pause": 1,
    "syslog": false,
    "verbose": {{ verbose_level | lower }},
    "workers": "password",
    "pools": {{ pools | to_nice_json(indent=2) }}
}
